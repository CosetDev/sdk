{
    "namespace": "@coset-dev",
    "name": "@coset-dev/sdk",
    "version": "1.0.6",
    "type": "module",
    "main": "./dist/cjs/coset.cjs",
    "module": "./dist/esm/coset.js",
    "types": "./dist/src/coset.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/src/coset.d.ts",
                "default": "./dist/esm/coset.js"
            },
            "require": {
                "types": "./dist/src/coset.d.ts",
                "default": "./dist/cjs/coset.cjs"
            }
        },
        "./node": {
            "types": "./dist/src/coset.d.ts",
            "default": "./dist/cjs/coset.cjs"
        },
        "./browser": {
            "types": "./dist/src/coset.d.ts",
            "default": "./dist/esm/coset.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsx esbuild.ts",
        "prepublishOnly": "npm run build",
        "test": "jest"
    },
    "authors": [
        {
            "name": "Halil Beycan"
        },
        {
            "name": "Ozan Anda√ß"
        }
    ],
    "license": "MIT",
    "homepage": "https://github.com/CosetDev/sdk",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CosetDev/sdk.git"
    },
    "bugs": {
        "url": "https://github.com/CosetDev/sdk/issues"
    },
    "dependencies": {
        "@x402/core": "^2.1.0",
        "@x402/evm": "^2.1.0",
        "@x402/fetch": "^2.1.0",
        "json-stable-stringify": "^1.3.0",
        "ts-node": "^10.9.2",
        "viem": "^2.43.3"
    },
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "dotenv": "^17.2.3",
        "esbuild": "^0.27.2",
        "esbuild-plugin-tsc": "^0.5.0",
        "fast-glob": "^3.3.3",
        "jest": "^30.2.0",
        "ts-jest": "^29.4.6"
    }
}